{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/dist/index.js"
        }],
        "builds": [
            {
                "src": "dist/index.js",
                "use": "@vercel/node"
            }
        ],
        "routes": [
            {
                "src": "/(.*)",
                "dest": "dist/index.js"
            }
        ]
    }